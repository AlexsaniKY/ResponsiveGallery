@model ResponsiveGallery.Domain.GalleryImage
<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 gallery-image-container">
    <div class="gallery-image-inner-container">
        <img src=@Model.ImageURL alt=@Html.DisplayFor(model => model.Title) class="gallery-image"/>
        <p class="gallery-image-title">@Html.DisplayFor(model => model.Title)</p>

    </div>
</div>